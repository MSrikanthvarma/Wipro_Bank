package org.com.example.test

import org.scalatest._
import collection.mutable.Stack
import org.scalatest.Rerunner._

class SquareRootTest extends FlatSpec {

  "Square root" should "work for values greater than zero" in {
    val sqrt = new Stack[Int]
    sqrt.push(1)
    assert(sqrt.pop() == 1)
  }
}